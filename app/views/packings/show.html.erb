<% content_for :header do %>
  <section class="content-header">
    <div class="row">
      <div class="col-xs-12">
        <ol class="breadcrumb" style="font-size:medium;">
          <li><a href="/dashboard"><i class="fa fa-dashboard"></i> 首页 </a></li>
          <li class="active">市级装烟管理</li>
          <li class="active">县级装烟统计</li>
        </ol>
      </div>
    </div>
  </section>
<% end %>

<% content_for :content do %>
  <p style="display: none;" class="code"><%=params[:code]%></p>
  <section class="content" style="margin-top:-25px;" >
    <div class="row">
      <div class="col-md-12">
        <div class="box box-widget">
          <div class="box-body">
            <table class="table table-striped table-hover" id="tTrolleys">
              <thead>
                <tr>
                  <th rowspan="2"></th>
                  <th rowspan="2">装烟量</th>
                  <th rowspan="2">杆数</th>
                  <th colspan="2">编竿分类(杆)</th>
                  <th colspan="2">杆夹分类(杆)</th>
                  <th colspan="2">分类装烟情况</th>
                  <th colspan="4">均匀性</th>

                </tr>
                <tr>
                  <th>同质</th>
                  <th>混编</th>

                  <th>均匀</th>
                  <th>不均匀</th>

                  <th>正确</th>
                  <th>不正确</th>

                  <th>均匀</th>
                  <th>前密后稀</th>
                  <th>前稀后密</th>

                  <th></th>
                </tr>
              </thead>
            </table>
          </div>
        </div>
      </div>
    </div>
  </section>
<% end %>